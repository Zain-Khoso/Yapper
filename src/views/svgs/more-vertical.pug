mixin moreVertical(className, fillClass, strokeClass)
  svg(viewbox='0 0 24 24' fill='none' xmlns='http://www.w3.org/2000/svg' class=className)
    g#SVGRepo_bgCarrier(stroke-width='0')
    g#SVGRepo_tracerCarrier(stroke-linecap='round' stroke-linejoin='round')
    g#SVGRepo_iconCarrier
      circle(cx='12' cy='6' r='1.5' class=fillClass)
      circle(cx='12' cy='12' r='1.5' class=fillClass)
      circle(cx='12' cy='18' r='1.5' class=fillClass)
